{
  "name": "perfect-immutable",
  "version": "1.4.0",
  "description": "Library to provide immutable methods (like immutable set similar to lodash's _.set) on standard JS objects",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lukasz-pluszczewski/perfect-immutable"
  },
  "main": "lib/perfect-immutable.js",
  "module": "lib/perfect-immutable.mjs",
  "jsnext:main": "lib/perfect-immutable.mjs",
  "scripts": {
    "prepublish": "NODE_PATH=src npm run build",
    "prebuild": "rimraf ./lib",
    "build": "NODE_PATH=src rollup -c",
    "watch": "NODE_PATH=src rollup -c -w",
    "lint": "esw rollup.config.js src test --color",
    "test": "npm run mochatest --silent",
    "test:watch": "npm run mochatest -- --watch",
    "mochatest": "NODE_PATH=src NODE_ENV=test nyc --reporter=html mocha tools/testSetup.js \"./test/**/*.js\""
  },
  "keywords": [
    "immutable",
    "javascript",
    "plain-object"
  ],
  "author": "≈Åukasz Pluszczewski",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babelrc-rollup": "3.0.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "eslint": "4.13.1",
    "eslint-config-brainhub": "1.6.3",
    "eslint-watch": "3.1.3",
    "mocha": "4.0.1",
    "nyc": "11.4.1",
    "rimraf": "2.6.2",
    "rollup": "0.52.1",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.0",
    "sinon": "4.1.3"
  },
  "dependencies": {
    "lodash": "4.17.4"
  }
}
